  "name": "",
  "private": true,
  "scripts": {
    "build": "concurrently tsc \"vite build\"",
    "commit": "git-cz",
    "dev": "vite",
    "format": "prettier \"**/*.{css,html,js,jsx,json,less,md,scss,ts,tsx,yml}\" -c -w",
    "husky:commit-msg": "husky add \".husky/commit-msg\"  \"npx --no -- commitlint --edit ${1}\"",
    "husky:pre-commit": "husky add \".husky/pre-commit\" \"npx --no-install lint-staged\"",
    "init": "concurrently npm:init:*",
    "init:husky": "concurrently \"husky install\" npm:husky:*",
    "init:pwa": "pwa-asset-generator \"src/assets/images/pwa/logo.svg\" \"public/assets/images/pwa\" -f -i index.html -m manifest.json -o false -t png -v \"assets/images/pwa\"",
    "init:ssl": "concurrently \"mkdirp config/ssl\" \"mkcert -cert-file config/ssl/cert.pem -key-file config/ssl/key.pem localhost\"",
    "lint": "concurrently npm:lint:*",
    "lint:eslint": "eslint \"**/*.{js,jsx,ts,tsx}\"",
    "lint:stylelint": "stylelint \"**/*.{css,less,scss}\"",
    "prebuild": "concurrently npm:format npm:lint",
    "preview": "vite preview"
  },
  "type": "module",
  "version": "0.0.0"
}
